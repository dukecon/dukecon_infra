[server]
; domain = grafana
root_url = https://<%= @grafana_base_url %>/grafana/

[auth.generic_oauth]
name = <%= @keycloak_base_url %>
enabled = true
client_id = grafana
client_secret = <%= @keycloak_client_secret %>
scopes = openid profile email
email_attribute_name = email:primary
auth_url = https://<%= @keycloak_base_url %>/auth/realms/Infrastructure/protocol/openid-connect/auth
token_url = https://<%= @keycloak_base_url %>/auth/realms/Infrastructure/protocol/openid-connect/token
api_url = https://<%= @keycloak_base_url %>/auth/realms/Infrastructure/protocol/openid-connect/userinfo
allowed_domains = gmx.de gmail.com doag.org gmx.org
allow_sign_up = true
